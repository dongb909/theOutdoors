<%- include("../partial/header")%>
<div class="container">
	<h1><%=location.name%></h1>
	<img src="<%=location.image%>" style="width:500px;">
	<p><%=location.description%></p>

<% location.comments.forEach((comment)=>{ %>
	<p>
		<strong><%=comment.author%></strong> - <%= comment.text %>
	</p>
<% }) %>
</div>


